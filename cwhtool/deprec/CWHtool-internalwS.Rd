\name{chlib-internal}
\alias{trigrid}
\alias{S.rotate.default}
\title{Internal chlib functions}
\description{
  Internal chlib functions
  \code{S.rotate.default} rotates the loadings and  scores  of  an  object  of  class
       "factanal" or "princomp".
}
\usage{
 trigrid(grid = FALSE)
 S.rotate.default(x, rotation = NULL, orthogonal = TRUE, parameters = NULL, normalize = TRUE)
}
\details{
  These functions are not to be called directly by the user.
}
\arguments{
  \item{x}{either a formula or a numeric matrix or an object that can be coerced to a numeric matrix}
  \item{rotation}{}
  \item{orthogonal}{}
  \item{parameters}{}
  \item{normalize}{}
  \item{grid}{}
}
\keyword{misc}
